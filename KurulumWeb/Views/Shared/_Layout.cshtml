    <script>
   document.addEventListener("DOMContentLoaded", function () {
    var fileInput = document.getElementById("file");
    var browseButton = document.getElementById("btnBrowse");
    var cancelButton = document.getElementById("btnCancel");
    var uploadButton = document.getElementById("btnUpload");
    var fileDropArea = document.querySelector(".file-drop-area");
    var uploadedFileName = document.getElementById("uploadedFileName");

    fileDropArea.addEventListener("dragover", function (e) {
        e.preventDefault();
        e.stopPropagation();
        fileDropArea.classList.add("dragover");
    });

    fileDropArea.addEventListener("dragleave", function (e) {
        e.preventDefault();
        e.stopPropagation();
        fileDropArea.classList.remove("dragover");
    });

    fileDropArea.addEventListener("drop", function (e) {
        e.preventDefault();
        e.stopPropagation();
        fileDropArea.classList.remove("dragover");

        var file = e.dataTransfer.files[0];
        uploadedFileName.innerText = file.name;
        fileInput.files = e.dataTransfer.files;
    });

    browseButton.addEventListener("click", function () {
        fileInput.click();
    });
    fileInput.addEventListener("change", function () {
        uploadedFileName.innerText = fileInput.files[0].name;
    });

    uploadButton.addEventListener("click", function () {
        var file = fileInput.files[0];
        if (file) {
            handleFile(file);
        } else {
            alert("@Resources.L281");
        }
    });
       cancelButton.addEventListener("click", function () {
           // Dosya seçimi silme işlemi
           fileInput.value = null; // Dosya seçimi sıfırlanıyor
           uploadedFileName.innerText = ""; // Dosya adını temizle
       });
});

        function showPopupMessage(message, success) {
            var alertDiv = document.getElementById("alertmessage");
            var alertIcon = success ? '<i class="fe fe-thumbs-up"></i>' : '<i class="fe fe-alert-circle"></i>';

            alertDiv.innerHTML = '<div class="alert ' + (success ? 'alert-success' : 'alert-danger') + '" role="alert">' +
                '<span class="alert-inner--icon">' + alertIcon + '</span>' +
                '<span class="alert-inner--text"><strong>' + (success ? 'Success!' : 'Error!') + '</strong> ' + message + '</span>' +
                '</div>';

            alertDiv.style.display = "block";

            setTimeout(function () {
                alertDiv.style.display = "none";
                location.reload();
            }, 5000); // Mesajı 5 saniye sonra gizle
        }

function handleFile(file) {
    var formData = new FormData();
    formData.append("file", file);

    $.ajax({
        url: "@Url.Action("UploadExcel", "Home")", // MVC'nin ilgili yolunu düzenlemelisiniz
        type: "POST",
        data: formData,
        processData: false,
        contentType: false,
        success: function (response) {
            if (response.success) {
                showPopupMessage("@Resources.L282", true);
                var uploadedFileName = response.fileName;
                $("#uploadedFileName").text("@Resources.L283 " + uploadedFileName);

            } else {
                showPopupMessage("@Resources.L284", false);
            }
        },
        error: function (xhr, status, error) {
            alert("@Resources.L285 " + error);
        }
    });
}
    </script>
